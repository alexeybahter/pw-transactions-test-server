{
  "name": "pw-server-2.0",
  "version": "1.0.0",
  "description": "Internal server for Fusion team",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --experimental-worker server.js",
    "prestart": "node ./node_modules/.bin/swagger-jsdoc -d ./utils/swaggerInit.js",
    "test": "npm run lint && jest",
    "lint": "eslint . -c .eslintrc.js --ext .js,.jsx --ignore-path .gitignore",
    "migrate": "sequelize db:migrate",
    "migration:undo": "sequelize db:migrate:undo",
    "migration:generate": "sequelize migration:generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "author": "Alexey A.",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "jest": "^24.9.0",
    "nodemon": "^1.17.5",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "change-case": "^3.0.2",
    "cookie-parser": "^1.4.3",
    "email-validator": "^2.0.4",
    "express": "^4.16.3",
    "express-validator": "^6.1.1",
    "file-system": "^2.2.2",
    "fs": "0.0.1-security",
    "glob": "^7.1.6",
    "husky": "^3.0.0",
    "jsonwebtoken": "^8.2.2",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "moment": "^2.22.2",
    "path": "^0.12.7",
    "pg": "^7.4.3",
    "require-directory": "^2.1.1",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.1.1",
    "swagger-jsdoc": "^3.4.0",
    "swagger-ui-express": "^4.1.1",
    "turndown": "^5.0.3",
    "web-push": "^3.3.3",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  }
}
